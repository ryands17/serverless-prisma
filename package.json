{
  "name": "serverless-prisma2",
  "version": "1.0.0",
  "author": {
    "name": "Ryan Dsouza"
  },
  "private": true,
  "scripts": {
    "dev": "run-p generate:prisma watch:* offline",
    "deploy": "cross-env NODE_ENV=production sls deploy",
    "offline": "dotenv -- sls offline",
    "db:save": "yarn prisma migrate dev --create-only --preview-feature",
    "db:migrate": "yarn prisma migrate dev --preview-feature",
    "seed": "dotenv -- ts-node prisma/seed.ts",
    "watch:nexus": "tsnd --transpile-only ./src/schema",
    "watch:ts": "tsc --watch",
    "generate:prisma": "yarn prisma generate",
    "prisma": "dotenv -- prisma"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/cli": "^2.13.1",
    "@prisma/client": "^2.13.1",
    "apollo-server-lambda": "^2.19.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.27.0"
  },
  "devDependencies": {
    "@types/node": "14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "husky": ">=4.3.6",
    "lint-staged": ">=10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "serverless": "^2.9.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  }
}
